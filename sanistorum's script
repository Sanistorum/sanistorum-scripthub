local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Xheptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("sanistorum's script hub", "GrapeTheme")

local Main = Window:NewTab("items and more")

local MainSection = Main:NewSection("items and more")

MainSection:NewButton("doritos", "eat them fatty.", function()
   local Item = game:GetObjects("rbxassetid://5125348322") [1]
Item.Parent = game.Players.LocalPlayer.Backpack
end)


MainSection:NewButton("soda", "drink if you need drink", function()
   local Item = game:GetObjects("rbxassetid://66071183") [1]
Item.Parent = game.Players.LocalPlayer.Backpack
end)


MainSection:NewButton("stop sign", "tell someone to stop", function()
   local Item = game:GetObjects("rbxassetid://1305561850") [1]
Item.Parent = game.Players.LocalPlayer.Backpack
end)


MainSection:NewButton("worm", "become sneke??? (rel)", function()
   loadstring(game:HttpGet(('https://pastefy.ga/tWBTcE4R/raw'),true))()
end)


MainSection:NewSlider("RUN SPED", "THE FLASH CAMERAMAN MODE", 1000, 10, function(s)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


MainSection:NewButton("admin cmd", "do anything you wanted in life", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)


MainSection:NewButton("doors crucifix (read desc)", "ONLY USE IN DOORS (credits to zepsyy)", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/ZepsyyCodesLUA/Utilities/main/Doors/Crucifix.lua'))()
end)


MainSection:NewButton("doors entity spawner", "ONLY USE IN DOORS (credits to plamenrock6789)", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/plamen6789/DoorsEntitySummonerGUI/main/EntityGUI'))()
end)


MainSection:NewSlider("Gravity", "Gives you more jump power.", 350, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)


MainSection:NewButton("Fake IP Grabber", "troll kids", function()
